  <h1 id="resultText"></h1>
  <script>

    let response = prompt("Can you be my girlfriend? Type Yes or No");
    let response = prompt("Can you be my husband? Type Yes or No");
    let outputText = "";

    if (response=="Yes"){
      outputText = "Date tayo sa Saturday!";
      outputText = "bottom Kita bukas";
    }else if(response=="No"){
      outputText = "sorry ganto lang ako";
      outputText = "wag kana ulit mag sinungaling";
    }else{
      outputText = "please type Yes or No!!";
    }
    
    document.getElementById("resultText").innerHTML = outputText;
  </script>
</body>
</html>
